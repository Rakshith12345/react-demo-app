apiVersion: apps/v1
kind: Deployment
metadata:
  name: minikube-deploy
spec:
  replicas: 2
  selector:
    matchLabels:
      app: react
  template:
    metadata:
      labels:
        app: react
    spec:
      containers:
        - name: react-app
          image: rakshithraghu/react-electricity-app:latest
          ports:
            - containerPort: 80
